{
  "name": "fcr-website",
  "version": "1.0.0",
  "scripts": {
    "build": "NODE_ENV=production concurrently \"pnpm:build-*\" && ELEVENTY_ENV=production eleventy",
    "build-css": "tailwindcss -i ./src/_assets/css/styles.css -o ./public/assets/css/styles.css --minify",
    "build-js": "esbuild ./src/_assets/js/scripts.js --outfile=./public/assets/js/scripts.js  --bundle --target=es2018 --minify",
    "clean": "del public",
    "start": "NODE_ENV=development concurrently  \"ELEVENTY_ENV=development eleventy --serve\"  \"pnpm:watch-*\"",
    "watch-css": "tailwindcss -i ./src/_assets/css/styles.css -o ./public/assets/css/styles.css -w",
    "watch-js": "esbuild ./src/_assets/js/scripts.js --outfile=./public/assets/js/scripts.js --bundle --target=es2018 --watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-upgrade-help": "^2.0.5",
    "@aloskutov/eleventy-plugin-external-links": "^1.4.6",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/linkify-it": "^3.0.2",
    "@types/markdown-it": "^12.2.3",
    "@types/mdurl": "^1.0.2",
    "@types/nodemailer": "^6.4.8",
    "alpinejs": "^3.12.2",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.1.0",
    "del-cli": "^5.0.0",
    "@uncenter/eleventy-plugin-toc": "^1.0.3",
    "esbuild": "^0.17.19",
    "html-minifier": "^4.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-eleventy-img": "^0.10.1",
    "md5": "^2.3.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "@cloudflare/pages-plugin-sentry": "^1.1.1",
    "@googleworkspace/drive-picker-element": "^0.3.4",
    "cookie": "^0.5.0"
  },
  "packageManager": "pnpm@10.15.0"
}
